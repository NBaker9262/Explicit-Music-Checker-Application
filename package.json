{
  "name": "explicit-music-checker-application",
  "version": "1.0.0",
  "private": true,
  "description": "Spotify-based song request search and queue review app",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "cf:dev": "wrangler dev",
    "cf:deploy": "wrangler deploy",
    "cf:whoami": "wrangler whoami",
    "cf:deployments": "wrangler deployments list --json",
    "cf:tail": "wrangler tail",
    "cf:secret:list": "wrangler secret list --format pretty",
    "cf:secret:put": "wrangler secret put",
    "cf:secret:delete": "wrangler secret delete",
    "cf:secret:bulk": "wrangler secret bulk",
    "cf:d1:list": "wrangler d1 list",
    "cf:migrate:list:local": "wrangler d1 migrations list music-queue --local",
    "cf:migrate:list:remote": "wrangler d1 migrations list music-queue --remote",
    "cf:migrate:local": "wrangler d1 migrations apply music-queue --local",
    "cf:migrate:remote": "wrangler d1 migrations apply music-queue --remote"
  },
  "dependencies": {
    "express": "^4.21.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "wrangler": "^4.53.1"
  }
}
